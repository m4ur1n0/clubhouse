
<% content_for :title, "Events" %>

<h1>Events</h1>

<section id="events">
  <% @events.each do |event| %>
    <%= render event %>
    <p>
      <%= link_to "Show this event", event, class: "btn btn-outline-secondary btn-sm" %>
    </p>
  <% end %>
</section>

<%= link_to "New event", new_event_path %>
